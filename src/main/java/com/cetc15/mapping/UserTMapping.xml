<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cetc15.dao.UserTMapping">
	
	<select id="getUserById" parameterType="int" resultType="com.cetc15.entity.UserT">
		select 
			* 
		from 
			user_t
		where 
			id = #{id}	
	</select>
	
</mapper>